.toolBar{
    display: flex;
    margin-bottom: 1px;
    height: 46px;
    background-color: white;
}

.toolBarItem{
    flex: 1;
    text-align: center;
    line-height: 46px;
    height: 46px;
    color: #515050;
    font-size: 17px;
}

.selectItem {
    color: #d140b5;
}

.line {
    background-color: #d140b5;
    height: 1px;
    display: inline-block;
    margin-left: auto;
    margin-right: auto;
    position: relative;
    top: -6px;
}